{
  "name": "ng-named-routes",
  "version": "0.1.5",
  "description": "Service to set and get named routes for angular 4+ router",
  "main": "dist/named-routes.service.js",
  "types": "dist/named-routes.service.d.ts",
  "repository": "git@github.com:Gregcop1/angular-named-routes.git",
  "author": "Gr√©gory Copin <gregcop1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular 2",
    "angular 4",
    "router",
    "named route"
  ],
  "scripts": {
    "build": "rm -Rf dist && tsc",
    "lint": "tslint -c tslint.json 'src/*.ts'",
    "pretest": "yarn lint",
    "test": "$(npm bin)/jest"
  },
  "devDependencies": {
    "@types/jest": "^21.1.3",
    "@types/node": "^8.0.34",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.2",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "uglify-js": "^3.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.spec\\.ts$"
  }
}
